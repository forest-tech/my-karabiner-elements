{
   "title": "Meh キー関連の設定 (JIS配列用)",
   "rules": [
      {
         "description": "CapsLock → Meh (Ctrl + Option + Shift)",
         "manipulators": [
            {
               "from": {
                  "key_code": "caps_lock"
               },
               "to": [
                  {
                     "key_code": "right_shift",
                     "modifiers": [
                        "right_control",
                        "right_option"
                     ]
                  }
               ],
               "type": "basic"
            }
         ]
      },
      {
         "description": "英数 -> Lcmd, かな -> Meh (Ctrl + Option + Shift).",
