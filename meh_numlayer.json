            {
               "from": {
                  "key_code": "japanese_eisuu",
                  "modifiers": {
                     "optional": [
                        "any"
                     ]
                  }
               },
               "to": [
                  {
                     "key_code": "left_command",
                     "lazy": true
                  }
               ],
               "to_if_alone": [
                  {
                     "key_code": "japanese_eisuu"
                  }
               ],
               "type": "basic"
            },
            {
               "from": {
                  "key_code": "japanese_kana",
                  "modifiers": {
                     "optional": [
                        "any"
                     ]
                  }
               },
               "to": [
                  {
                     "key_code": "right_shift",
                     "lazy": true,
                     "modifiers": [
                        "right_control",
                        "right_option"
                     ]
                  }
               ],
               "to_if_alone": [
                  {
                     "key_code": "japanese_kana"
                  }
               ],
               "type": "basic"
            }
         ]
